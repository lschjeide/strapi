{
  "kind": "collectionType",
  "collectionName": "movie_ratings",
  "info": {
    "singularName": "movie-rating",
    "pluralName": "movie-ratings",
    "displayName": "MovieRating"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "rating": {
      "type": "integer",
      "min": 1,
      "max": 10,
      "required": true
    },
    "movie": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::movie.movie",
      "inversedBy": "movie_ratings"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
